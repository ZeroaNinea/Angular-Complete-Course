{
  "ci": {
    "collect": {
      "startServerCommand": "npm start",
      "url": ["http://localhost:4200"],
      "numberOfRuns": 1
    },
    "assert": {
      "bf-cache": "warn",
      "preset": "lighthouse:recommended"
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
